  <POU Name="MAIN" Id="{3b2e30a5-fd08-4af5-bac7-164098d87b7b}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM MAIN
VAR
	fbMover							: MC_PlanarMover;
	afbAxis							: ARRAY [1..6] OF AXIS_REF;
